              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"owner@example.com\",\n  \"password\": \"password123\",\n  \"name\": \"John Doe\",\n  \"phone\": \"+1234567890\",\n  \"organizationName\": \"Dental Care Clinic\",\n  \"organizationAddress\": \"123 Main St, City, State 12345\",\n  \"organizationPhone\": \"+1234567891\",\n  \"organizationEmail\": \"clinic@example.com\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            },
            "description": "Register a new organization owner with organization details"
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"owner@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            },
            "description": "Login with email and password"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Extract token from cookie and set as environment variable",
                  "const cookies = pm.response.headers.get('Set-Cookie');",
                  "if (cookies) {",
                  "    const tokenCookie = cookies.split(';').find(c => c.trim().startsWith('token='));",
                  "    if (tokenCookie) {",
                  "        const token = tokenCookie.split('=')[1];",
                  "        pm.environment.set('authToken', token);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/auth/profile",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "profile"]
            },
